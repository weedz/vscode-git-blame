{
    "name": "vscode-git-blame",
    "version": "1.1.0",
    "description": "",
    "publisher": "weedzcokie",
    "type": "commonjs",
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "pnpm run build",
        "package": "vsce package",
        "build": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "activationEvents": [],
    "contributes": {
        "commands": [
