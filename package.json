            {
                "command": "extension.git-blame.toggle-inline-hints",
                "title": "Git Blame: Toggle inline blame"
            }
        ]
    },
    "engines": {
        "vscode": "^1.80.0"
    },
    "devDependencies": {
        "@types/node": "^20.4.2",
